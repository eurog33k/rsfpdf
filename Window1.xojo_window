#tag Events Example3
	#tag Event
		Sub Action()
		  dim pdf as fpdf
		  dim j,i as integer
		  
		  me.Enabled = False
		  
		  pdf = new FPDF("P")
		  pdf.AddPage()
		  
		  pdf.SetFont("Courier","",8)
		  
		  //Cabecera
		  for i = 1 to 4
		    pdf.Cell(40,7,"Cabecera " + str(i),1)
		  next i
		  
		  pdf.Ln()
		  
		  //Datos
		  for j = 1 to 10
		    
		    for i = 1 to 4
		      pdf.cell(40,6,"Item " + str(i),1)
		    next i
		    
		    pdf.ln()
		    
		  next j
		  
		  pdf.Output(me.Name + ".pdf")
		  
		  me.Enabled = true
		  
		End Sub
	#tag EndEvent
#tag EndEvents
