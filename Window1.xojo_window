#tag Events Example1
	#tag Event
		Sub Action()
		  dim pdf as fpdf
		  dim j,i as integer
		  
		  me.Enabled = False
		  
		  if EditField1.Text = "" then
		    MsgBox "You must write some text"
		    EditField1.SetFocus
		    return
		  end if
		  
		  pdf = new FPDF("P","mm","letter")
		  pdf.AddPage()
		  pdf.SetFont("Arial","B",16)
		  pdf.Cell(40,10,EditField1.Text)
		  pdf.Output(me.Name + ".pdf")
		  
		  me.Enabled = true
		  
		End Sub
